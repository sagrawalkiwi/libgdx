<project name="gdx-freetype-natives" basedir="." default="all">
	<target name="clean">
		<ant antfile="build-ios32.xml" target="clean"/>

	</target>
	
	<target name="compile-natives">
		<ant antfile="build-ios32.xml"/>

	</target>
	
	<target name="pack-natives">
		<jar destfile="../libs/gdx-freetype-natives.jar">
			<fileset dir="../libs/ios32" includes="gdx-freetype.dll"/>

		</jar>
	</target>

	<target name="all" depends="compile-natives,pack-natives"/>
</project>
